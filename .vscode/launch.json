    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Tape Tests",
            "type": "node2",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/.bin/tape",
            "args": [
                "src/trie.spec.js"
            ],
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "npm script: pretest",
            "type": "node2",
            "request": "launch",
            "program": "${workspaceRoot}/npm-scripts/pretest.js",
            "cwd":"${workspaceRoot}"
        }
    ]
}