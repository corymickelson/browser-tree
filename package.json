{
  "name": "abs",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint:watch": "esw --ignore-pattern \"node_modules\" .",
    "prewatch": "node npm-scripts/pretest.js",
    "watch": "tape-watch src/**.spec.js",
    "postwatch": "rm src/tmp.tests.js",
    "build:index": "rollup src/index.js -o src/index.build.js -f \"cjs\"",
    "preintegTest": "npm run build:index && node npm-scripts/pretest.js",
    "integTest": "browserify src/tmp.tests.js | tape-run --browser=\"chrome\" --render=\"tap-spec\"",
    "postintegTest": "node npm-scripts/posttest.js && rm src/tmp.tests.js"
  },
  "repository": {
    "url": "git+https://github.com/corymickelson/signature.git",
    "type": "git"
  },
  "author": "Cory Mickelson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/corymickelson/signature/issues"
  },
  "homepage": "https://github.com/corymickelson/signature#readme",
  "dependencies": {
    "dexie": "^1.5.0",
    "webcomponentsjs": "^1.0.2"
  },
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "browserify": "*",
    "eslint-watch": "^2.1.14",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "rollup": "^0.36.3",
    "rollup-plugin-json": "^2.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "tape-run": "^2.1.4"
  }
}
